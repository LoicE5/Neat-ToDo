{
    "version": 2,
    "builds": [
        {
            "src": "/",
            "use": "@vercel/node",
            "config": { "includeFiles": ["server/build/**"] }
        },
        {
            "src": "frontend/next.config.js",
            "use": "@vercel/next"
        }
    ],
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
    
}